    "postcss": "^5.0.21",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.6.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.2",
    "pre-push": "^0.1.1",
    "react-hot-loader": "^3.0.0-alpha.8",
    "style-loader": "^0.13.1",
